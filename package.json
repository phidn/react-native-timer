{
  "name": "mindful-checkin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && gradlew clean",
    "android:bundleRelease": "cd android && gradlew bundleRelease",
    "android:openReleaseFolder": "cd android\\app\\build\\outputs\\bundle\\release && start .",
    "android:releaseCode": "react-native-version --never-amend",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:rc": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "emulator": "cd C:/Users/phidn/AppData/Local/Android/Sdk/emulator && emulator.exe -avd Pixel_4_API_29",
    "phone": "cd C:/Users/phidn/AppData/Local/Android/Sdk/emulator && emulator.exe -avd Pixel_4_API_29",
    "tablet7in": "cd C:/Users/phidn/AppData/Local/Android/Sdk/emulator && emulator.exe -avd Nexus_7_API_33",
    "tablet10in": "cd C:/Users/phidn/AppData/Local/Android/Sdk/emulator && emulator.exe -avd Nexus_10_API_33",
    "device": "adb -s 62d6ca7c reverse tcp:8081 tcp:8081",
    "postinstall": "patch-package",
    "trans": "jsontt src/translations/common/en.json",
    "deepl": "node ./server/deeplServer.js",
    "fixTrans": "nodemon ./server/fixTranslate.js",
    "bootsplash": "react-native generate-bootsplash ./src/assets/images/logo.png"
  },
  "nodemonConfig": {
    "ignore": [
